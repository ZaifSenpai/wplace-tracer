    "dev-firefox": "webpack --progress --config webpack.dev.js --env=browser=firefox",
    "build-all": "npm run build-chrome && npm run build-firefox",
    "build-chrome": "webpack --progress --config webpack.prod.js --env=browser=chrome",
    "build-firefox": "webpack --progress --config webpack.prod.js --env=browser=firefox"
  },
